{
  "name": "ansyn-event-drops",
  "version": "0.4.7",
  "description": "A time based / event series interactive visualization using d3.js. Use drag and zoom to navigate in time.",
  "main": "dist-es5/index.js",
  "files": [
    "dist/",
    "dist-es5/",
    "style.css",
    "*.md",
    "src",
    "*.ts"
  ],
  "repository": {
